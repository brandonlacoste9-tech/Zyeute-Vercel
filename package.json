{
  "name": "zyeute",
  "version": "0.1.0",
  "description": "Quebec's AI-powered social platform with marketplace and autonomous agents",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
