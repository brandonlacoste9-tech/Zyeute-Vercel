    "@vitest/ui": "^1.0.4",
    "@vitest/coverage-v8": "^1.0.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.1.5",
    "jsdom": "^23.0.0",
    "playwright": "^1.40.1",
    "@prisma/cli": "^5.7.0",
    "prisma": "^5.7.0",
    "tailwindcss": "^4.0.0",
    "postcss": "^8.4.31",
    "autoprefixer": "^10.4.16"
