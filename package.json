    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "stripe": "^14.0.0",
    "openai": "^4.20.0",
    "@fal-ai/serverless-client": "^0.9.0",
    "@prisma/client": "^5.7.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.0",
    "framer-motion": "^12.0.0",
    "react-icons": "^5.0.1",
